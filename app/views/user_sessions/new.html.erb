<div class="container">
  <div class="row">
   <div>

    <h1>Log In</h1>

     <%= form_tag log_in_path do %>
      
      <div class="form-group">
        <%= label_tag :email %>
        <%= text_field_tag :email %>
      </div>

       <div class="form-group">
        <%= label_tag :password %>
        <%= password_field_tag :password %>
      </div>    


      <div>
      <%= submit_tag 'Log In' %>
      </div>

    <% end %>

  </div>
 </div>
</div>