<!DOCTYPE html>
<html ng-app="prinit">
<head>
  <title>Prinit</title>
  <%= stylesheet_link_tag  'application' %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
</head>
<body ng-controller="MainController as main" ng-class="{true: 'body-pushed'}[main.pushmenu]">



<%= render 'shared/header' %>

<div id="view-container" class="container-fluid" ng-click="main.pushmenu = false">

<%= yield %>

</div>

</body>
</html>
